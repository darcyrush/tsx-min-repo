{
  /* Visit https://aka.ms/tsconfig.json to read more about this file */
  "files": [
    "main.ts"
  ],
  "compilerOptions": {
    "experimentalDecorators": true,
    "strict": true,
    "target": "ES2022",
    "module": "commonjs",
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "emitDecoratorMetadata": true,
    "moduleResolution": "node",
  }
}